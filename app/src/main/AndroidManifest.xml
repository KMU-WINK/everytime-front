<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wink.knockmate">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Knockmate"
        android:usesCleartextTraffic="true">
        <activity android:name=".CharacterChoiceActivity" />
        <activity android:name=".WorkThroughActivity" />
        <activity android:name=".TempActivity" />
        <activity android:name=".AppInfoActivity" />
        <activity android:name=".InquiryActivity" />
        <activity android:name=".NotationActivity" />
        <activity android:name=".InviteGroupMemActivity" />
        <activity android:name=".CreateGroupActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".TermsActivity" />
        <activity android:name=".NicknameActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".LogInActivity" />
        <activity android:name=".EditProfileActivity" />
        <activity android:name=".EditEmailActivity" />
        <activity android:name=".EditPasswordActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".KnockmateActivity" />
        <activity android:name=".ModifyScheduleActivity" />
        <activity
            android:name=".TestMain"
            android:exported="true"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>